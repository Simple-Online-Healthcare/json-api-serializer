{
  "name": "simple-online-healthcare/json-api-serializer",
  "description": "Serialise Doctrine entities to JSON:API for Laravel 9.x",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Josh Murray",
      "email": "josh.murray@simpleonlinehealthcare.com"
    },
    {
      "name": "Adam Campbell",
      "email": "adam.campbell@simpleonlinehealthcare.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SimpleOnlineHealthcare\\JsonApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "require": {
    "laravel/framework": "^9.19",
    "simple-online-healthcare/contracts": "^1.5.0",
    "symfony/serializer": "^6.3",
    "symfony/property-access": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Simple-Online-Healthcare/contracts"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "SimpleOnlineHealthcare\\JsonApi\\SerializerServiceProvider"
      ]
    }
  }
}
